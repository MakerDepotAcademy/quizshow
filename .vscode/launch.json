  "version": "0.2.0",
  "configurations": [
  {
    "type": "node",
    "request": "launch",
    "name": "Electron Main",
    "runtimeExecutable": "${workspaceFolder}/ETest/node_modules/.bin/electron",
    "program": "${workspaceFolder}/ETest/index.js",
    "cwd": "${workspaceFolder}/ETest"
  },
    {
      "name": "Python: Quiz Show Game Play",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/QuizShowGamePlay.py",
      "console": "integratedTerminal"
    }
  ]
}